<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="480.0" prefWidth="736.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ucf.assignments.ExpandItemController">
   <children>
      <Button layoutX="670.0" layoutY="24.0" mnemonicParsing="false" onAction="#backButtonClicked" text="Back" />
      <VBox fillWidth="false" layoutX="14.0" layoutY="58.0" prefWidth="687.0" spacing="15.0">
         <children>
            <Label text="Due Date:">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Label>
            <Label fx:id="dueDateLabel" prefWidth="100.0" text="Label" />
            <Label text="Item Name:">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Label>
            <Label fx:id="itemNameLabel" maxWidth="1.7976931348623157E308" prefHeight="65.0" prefWidth="686.0" text="Label" />
            <Label text="Item Description:">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Label>
            <Label fx:id="itemDescriptionLabel" prefHeight="65.0" prefWidth="687.0" text="Label" wrapText="true" />
            <Label text="Item Status:">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Label>
            <Label fx:id="itemStatusLabel" prefHeight="17.0" prefWidth="100.0" text="Label" wrapText="true" />
         </children>
      </VBox>
      <Label layoutX="318.0" layoutY="27.0" text="Expanded Item">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
